            android:layout_marginStart="160dp" android:layout_marginEnd="179dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="414dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/timeDisplayTextView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="165dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="162dp" android:layout_marginBottom="61dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView" android:layout_marginEnd="191dp"
            app:layout_constraintEnd_toEndOf="parent"/>
